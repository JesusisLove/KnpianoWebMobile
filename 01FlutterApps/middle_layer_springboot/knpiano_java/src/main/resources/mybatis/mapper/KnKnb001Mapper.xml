<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liu.springboot04web.mapper.KnBnk001Mapper">

    <select id="getInfoList" resultType="com.liu.springboot04web.bean.KnBnk001Bean">
        SELECT * FROM t_mst_bank
    </select>

    <select id="getInfoById" parameterType="String" resultType="com.liu.springboot04web.bean.KnBnk001Bean">
        SELECT * FROM t_mst_bank WHERE bank_id = #{id}
    </select>

    <insert id="insertInfo" parameterType="com.liu.springboot04web.bean.KnBnk001Bean">
        <!-- INSERT INTO t_mst_bank (bank_id, bank_name, del_flg, create_date, update_date) 
        VALUES (#{bankId}, #{bankName}, #{delFlg}, #{createDate}, #{updateDate}) -->
        INSERT INTO t_mst_bank (bank_id, bank_name) 
        VALUES (#{bankId}, #{bankName})
    </insert>

    <update id="updateInfo" parameterType="com.liu.springboot04web.bean.KnBnk001Bean">
        UPDATE t_mst_bank
        SET bank_name=#{bankName}, del_flg=#{delFlg}, create_date=#{createDate}, update_date=#{updateDate} 
        WHERE bank_id=#{bankId}
    </update>

    <delete id="deleteInfo" parameterType="String">
        DELETE FROM t_mst_bank WHERE bank_id = #{id}
    </delete>

    <select id="getNextSequence" parameterType="map" statementType="CALLABLE">
        {
            #{parm_out,mode=OUT,jdbcType=INTEGER} = call nextval(#{parm_in,mode=IN})
        }
    </select>
</mapper>
